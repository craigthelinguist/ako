(def id
  (lambda (x)
    (succ (pred x))
  )
)

(def not
  (lambda (x)
    (cond
      ((x) (nil))
      ((true) (true))
    )
  )
)

(def sgn
  (lambda (x)
    (cond
      ((> x 0) 1)
      ((< x 0) -1)
      ((true) 0)
    )
  )
)

(def exp
  (lambda (x y)
    (cond
      ((= y 0) 1)
      ((true) (mul x (exp x (pred y))))
    )
  )
)
